@model LandingPage.ViewModels.UserConsultInfoViewModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}
<form asp-action="GetConsult" asp-controller="Home" method="post">
    <div class="form-floating mb-3">
      <input asp-for="UserName" class="form-control" id="floatingInput" >
      <label for="floatingInput">Введите имя</label>
    </div>
    <div class="form-floating">
        <input asp-for="PhoneNumber" class="form-control" id="floatingPassword" >
        <label for="floatingPassword">Введите номер</label>
    </div>
     <div class="row mt-3">
       <div class="col text-center">
         <button type="submit" class="btn btn-primary">Отправить</button>
       </div>
     </div>
</form>

